{
    "name": "electivegroup/api-clients-php",
    "type": "symfony-bundle",
    "description": "Elective API Clients SDK",
    "keywords": ["elective", "symfony", "security", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Kris Rybak",
            "email": "kris@elective.io"
        }
    ],
    "require": {
        "php": "^7.1.13",
        "electivegroup/formatter-bundle": "^0",
        "symfony/http-client": "^4.4|^5",
        "symfony/http-foundation": "^4.4|^5"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "Elective\\ApiClients\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Elective\\ApiClients\\Tests\\": "tests/" }
    },
    "minimum-stability": "stable"
}
